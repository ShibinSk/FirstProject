<div class="colorlib-product">
	<div class="container">
		<div class="row">
			<div class="col-sm-8 offset-sm-2 text-center colorlib-heading colorlib-heading-sm">
				<h2>View All Products</h2>
			</div>
		</div>
		<div class="row">
			<div class="col-lg-3 col-xl-3">
				<div class="row">
					<div class="col-sm-12">
						<div class="side border mb-1">
							<h3>Brand</h3>
							<ul>
								<li><a href="#">Nike</a></li>
								<li><a href="#">Adidas</a></li>
								<li><a href="#">Puma</a></li>

							</ul>
						</div>
					</div>
					<div class="col-sm-12">
						<div class="side border mb-1">
							<h3>Size/Width</h3>
							<div class="block-26 mb-2">
								<h4>Size</h4>
								<ul>
									<li><a href="#">6</a></li>
									<li><a href="#">7</a></li>
									<li><a href="#">8</a></li>
									<li><a href="#">9</a></li>
									<li><a href="#"></a></li>
									<li><a href="#">10</a></li>
									<li><a href="#">11</a></li>
									<li><a href="#">12</a></li>
									<li><a href="#">13</a></li>
									
								</ul>
							</div>

						</div>
					</div>


				</div>
			</div>
			<div class="col-lg-6 col-xl-9">
				<div class="row row-pb-md">
					
					{{#each men}}
					<div class="col-md-4">
						<div class="wsk-cp-product">
							<div class="wsk-cp-img">
								<a href="/User/product-details?id={{this._id}}"><img src="/product-images/{{image.[0]}}"
										alt="Product" class="img-responsive" /></a>
							</div>
							{{#gt this.productOffer  this.categoryOffer}}
						<div class="bg-danger rounded text-white position-absolute start-0 top-0 m-1 py-1 px-2">
							
						{{this.productOffer}}% off</div>
						{{else}}
						<div class="bg-danger rounded text-white position-absolute start-0 top-0 m-1 py-1 px-2">
							
						{{this.categoryOffer}}% off</div>
						{{/gt}}
						
							<div class="wsk-cp-text">
								<a href="">
									<div class="category">
										<span>BUY</span>
									</div>
								</a>
								<div class="title-product">
									<h3>{{this.product}}</h3>

								</div>

								<div class="description-prod">
									<p>{{this.description}}</p>
								</div>
								<div class="card-footer">
									<p style="color: red ; font-weight:  500;"><s>₹ {{this.orginalprice}}</s> </p>
									<div style="color:green ; font-weight:  500;" class="wcf-left"><span class="price">₹ {{this.discountprice}}</span></div>
									<div class="wcf-right"><a href="/User/add-to-cart?_id={{this._id}}"
											class="buy-btn"><i class="fa-solid fa-cart-shopping"></i></a></div>
									<div class="wcf-right "><a href="#" class="buy-btn"><i
												class="fa-solid fafa fa-heart"></i></a>
									</div>
								</div>
							</div>
						</div>
					</div>
					{{/each}}

				</div>
			</div>
		</div>
